{
  "workbench.colorTheme": "GitHub Dark Colorblind (Beta)",
  "window.commandCenter": false,
  // Current Windows / else, change path and username.
  "solargraph.commandPath": "C:/Users/sudok/scoop/apps/ruby/current/gems/bin/solargraph",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none",
  "explorer.confirmDelete": false,
  // Current Windows / else, change path and username.
  "vscode-erb-beautify.executePath": "C:/Users/sudok/scoop/apps/ruby/current/gems/bin/htmlbeautifier",
  "redhat.telemetry.enabled": false,
  "jdk.telemetry.enabled": false,
  "workbench.settings.applyToAllProfiles": [],
  "mypy-type-checker.args": [
    "--allow-redefinition",
    "--check-untyped-defs",
    "--ignore-missing-imports",
    "--no-strict-optional",
    "--no-warn-no-return",
    "--warn-return-any",
    "--warn-redundant-casts"
  ],
  "mypy-type-checker.importStrategy": "fromEnvironment",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
  },
  "isort.args": [
    "--profile",
    "black"
  ],
  "flake8.args": [
    "--max-line-length=120",
    "--enable=W0614",
    "--ignore=W291, W292, W293"
  ],
  "editor.fontFamily": "HackGen, HackGen35, HackGen Console, HackGen35 Console",
  "editor.fontLigatures": false,
  "editor.suggestSelection": "first",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "chat.disableAIFeatures": true,
  "files.trimTrailingWhitespace": true,
  "cSpell.language": "en,en-GB",
  "cSpell.userWords": [
    "django",
    "Impl",
  ],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
  ],
  "python.analysis.indexing": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.diagnosticMode": "openFilesOnly",
  "python.analysis.include": [
    "piyo_app",
    "test_utils",
  ],
  "python.analysis.userFileIndexingLimit": 6000,
  "python.analysis.packageIndexDepths": [
    {
      "name": "django",
      "depth": 2
    },
    {
      "name": "rest_framework",
      "depth": 2
    },
    {
      "name": "fastapi",
      "depth": 2
    },
  ],
  "black-formatter.args": [
    "--line-length=120",
    "--skip-string-normalization"
  ],
  "python.testing.pytestEnabled": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "emmet.includeLanguages": {
    "django-html": "html",
  },
  "editor.inlineSuggest.enabled": true,
  "git.confirmSync": false,
  "files.associations": {
    "*.html": "html"
  },
  "git.autofetch": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportUnboundVariable": "information",
    "reportImplicitStringConcatenation": "warning"
  },
  "python.analysis.typeEvaluation.enableReachabilityAnalysis": true,
  "python.analysis.typeEvaluation.strictDictionaryInference": false,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "vsicons.dontShowNewVersionMessage": true
}